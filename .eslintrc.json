{
	"extends": [
		"plugin:jsx-a11y/recommended"
	],
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true,
		"jest/globals": true,
		"jasmine": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"jest",
		"jasmine",
		"jsx-a11y"
	],
	"rules": {
		"brace-style": [2, "1tbs", {
			"allowSingleLine": true
		}],
		"comma-dangle": [2, "never"],
		"comma-style": [2, "first", {
			"exceptions": {
				"ArrayExpression": true,
				"ObjectExpression": true
			}
		}],
		"complexity": [1, 6],
		"curly": 1,
		"eqeqeq": [2, "allow-null"],
		"max-statements": [2, 30],
		"no-shadow-restricted-names": 2,
		"no-use-before-define": 2,
		"radix": 1,
		"semi": 0,
		"space-infix-ops": 2,
		"strict": 1,
		"no-const-assign": "warn",
		"no-this-before-super": "warn",
		"no-undef": "warn",
		"no-unreachable": "warn",
		"no-unused-vars": "warn",
		"constructor-super": "warn",
		"valid-typeof": "warn",
		"react/jsx-uses-vars": [2],
		"react/jsx-uses-react": [2],
		"react/react-in-jsx-scope": 2,
		"react/prop-types": 1,
		"jsx-a11y/label-has-for": [2, {
			"components": ["label"],
			"required": {
				"every": ["id"]
			},
			"allowChildren": false
		}]
	}
}