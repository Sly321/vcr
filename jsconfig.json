{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true,
		"baseUrl": "src/",
		"paths": {
			"@service/*": [
				"service/*"
			],
			"@components/*": [
				"components/*"
			],
			"@scenes/*": [
				"scenes/*"
			],
			"@environment/*": [
				"environment/*"
			],
			"@model/*": [
				"model/*"
			]
		}
	}
}