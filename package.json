{
  "name": "vcr",
  "version": "0.9.56",
  "private": true,
  "homepage": "http://tv.slyox.de",
  "dependencies": {
    "autoprefixer": "7.2.3",
    "axios": "^0.17.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "file-loader": "1.1.6",
    "firebase": "^4.8.0",
    "font-awesome": "^4.7.0",
    "fs-extra": "5.0.0",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.2",
    "gulp-watch": "^4.3.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "21.2.1",
    "jest-junit": "^3.4.1",
    "moment": "^2.19.4",
    "npm-run-all": "^4.1.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^16.0.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "rxjs": "^5.5.5",
    "style-loader": "0.19.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build-css": "gulp less",
    "watch-css": "gulp less-watch",
    "start-dev-server": "NODE_ENV='development' ./node_modules/.bin/webpack-dev-server -d --config ./config/webpack.config.dev.js",
    "replace-title": "node scripts/cd-build-prepare.js",
    "build-js": "node scripts/build.js",
    "start": "npm run build-css && npm-run-all -p watch-css start-dev-server",
    "build": "npm run build-css && npm run build-js",
    "testv": "npm run build-css && node scripts/test.js --env=jsdom --verbose",
    "testw": "npm run build-css && node scripts/test.js --env=jsdom --verbose --watch",
    "test": "npm run build-css && node scripts/test.js --env=jsdom",
    "testc": "npm run build-css && node scripts/test.js --coverage --env=jsdom --forceExit",
    "storybook": "npm run build-css && npm-run-all -p watch-css start-storybook",
    "start-storybook": "start-storybook -p 6006 -c config/storybook",
    "build-storybook": "build-storybook"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/addon-links": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "@types/react-dom": "^16.0.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^5.4.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.5.1",
    "mini-css-extract-plugin": "^0.4.2",
    "raf": "^3.4.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-manifest-plugin": "^2.0.3"
  }
}