{
  "name": "vcr",
  "version": "0.9.56",
  "private": true,
  "homepage": "http://tv.slyox.de",
  "dependencies": {
    "axios": "^0.18.0",
    "firebase": "^5.4.1",
    "font-awesome": "^4.7.0",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "start": "NODE_ENV='development' ./node_modules/.bin/webpack-dev-server -d --config ./config/webpack.config.dev.js",
    "replace-title": "node scripts/cd-build-prepare.js",
    "build": "node scripts/build.js",
    "testv": "node scripts/test.js --env=jsdom --verbose",
    "testw": "node scripts/test.js --env=jsdom --verbose --watch",
    "test": "node scripts/test.js --env=jsdom",
    "testc": "node scripts/test.js --coverage --env=jsdom",
    "storybook": "npm run build-css && npm-run-all -p watch-css start-storybook",
    "start-storybook": "start-storybook -p 6006 -c config/storybook",
    "build-storybook": "build-storybook"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|less)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "@service/(.*)": "<rootDir>/src/service/$1",
      "@components/(.*)": "<rootDir>/src/components/$1",
      "@scenes/(.*)": "<rootDir>/src/scenes/$1",
      "@converter/(.*)": "<rootDir>/src/converter/$1",
      "@environment/(.*)": "<rootDir>/src/environment/$1",
      "@model/(.*)": "<rootDir>/src/model/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "globals": {
    "ts-jest": {
      "tsConfigFile": "tsconfig.json"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/addon-links": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^23.3.1",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "autoprefixer": "^9.1.3",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "dotenv": "4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "file-loader": "^2.0.0",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "jest-junit": "^3.4.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "moment": "^2.22.2",
    "npm-run-all": "^4.1.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "raf": "^3.4.0",
    "react-dev-utils": "^5.0.1",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.23.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-manifest-plugin": "^2.0.3"
  }
}
